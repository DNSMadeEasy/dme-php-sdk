{
    "name": "dns-made-easy/php-sdk",
    "description": "PHP SDK for the DNS Made Easy API",
    "type": "library",
    "require": {
        "ext-json": "^7.4",
        "php": "^7.4",
        "ext-mbstring": "^7.4",
        "psr/container": "^1.0",
        "psr/http-client-implementation": "^1.0",
        "guzzlehttp/psr7": "^1.7",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "guzzlehttp/guzzle": ">=6",
        "php-http/mock-client": "^1.4",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "monolog/monolog": "^2.1",
        "symfony/var-dumper": "^5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "DnsMadeEasy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DnsMadeEasyTest\\": "test/"
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jessica Smith",
            "email": "jsmith@tiggee.com"
        }
    ],
    "minimum-stability": "stable"
}
